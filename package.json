{
  "name": "components-ui-vuejs-test",
  "version": "1.0.2",
  "main": "./dist/umd.js",
  "module": "./dist/es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/components"
  ],
  "exports": {
    ".": {
      "import": "./dist/es.js",
      "require": "./dist/umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/components-ui-vuejs-test.css",
    "./package.json": "./package.json",
    "./components/LearnVueButton.vue": {
      "import": "./src/components/LearnVueButton.vue",
      "./components/*": "./src/components/*"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  }
}
